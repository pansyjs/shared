{
  "name": "@pansy/shared",
  "version": "1.1.0",
  "description": "pansy shared utils",
  "main": "_cjs/utils/index.js",
  "module": "utils/index.js",
  "types": "utils/index.d.ts",
  "exports": {
    ".": {
      "require": "./_cjs/utils/index.js",
      "import": "./utils/index.js",
      "types": "./utils/index.d.ts"
    },
    "./package.json": "./package.json",
    "./react": {
      "require": "./_cjs/react/index.js",
      "import": "./react/index.js",
      "types": "./react/index.d.ts"
    },
    "./types": "./types/index.d.ts",
    "./utils": {
      "require": "./_cjs/utils/index.js",
      "import": "./utils/index.js",
      "types": "./utils/index.d.ts"
    }
  },
  "sideEffects": [],
  "scripts": {
    "build": "redbud build && redbud run ./scripts/copyMetaFiles.ts",
    "test": "vitest",
    "prepare": "husky install",
    "tsc:check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "pansy",
    "utils"
  ],
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^18.7.11",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@umijs/fabric": "^3.0.0",
    "@umijs/utils": "^4.0.13",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "redbud": "^1.1.0",
    "semantic-release": "19.0.3",
    "typescript": "4.7.4",
    "vitest": "0.22.1"
  },
  "authors": [
    "wangxingkang <wang_xingkang@qq.com> (https://github.com/wangxingkang)"
  ],
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
