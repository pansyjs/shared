{
  "name": "@pansy/shared",
  "version": "1.8.0",
  "description": "pansy shared utils",
  "main": "_cjs/utils/index.js",
  "module": "utils/index.js",
  "types": "utils/index.d.ts",
  "exports": {
    ".": {
      "require": "./_cjs/utils/index.js",
      "import": "./utils/index.js",
      "types": "./utils/index.d.ts"
    },
    "./package.json": "./package.json",
    "./react": {
      "require": "./_cjs/react/index.js",
      "import": "./react/index.js",
      "types": "./react/index.d.ts"
    },
    "./tree": {
      "require": "./_cjs/tree/index.js",
      "import": "./tree/index.js",
      "types": "./tree/index.d.ts"
    },
    "./types": "./types/index.d.ts",
    "./utils": {
      "require": "./_cjs/utils/index.js",
      "import": "./utils/index.js",
      "types": "./utils/index.d.ts"
    }
  },
  "sideEffects": [],
  "scripts": {
    "build": "redbud build && redbud run ./scripts/copyMetaFiles.ts",
    "test": "vitest",
    "prepare": "husky install",
    "tsc:check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "pansy",
    "utils"
  ],
  "license": "MIT",
  "dependencies": {
    "react-is": "^18.2.0",
    "tree-lodash": "^0.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/git": "10.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-is": "^17.0.3",
    "@umijs/fabric": "^3.0.0",
    "@umijs/utils": "^4.0.55",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redbud": "1.6.0",
    "semantic-release": "19.0.5",
    "typescript": "4.9.5",
    "vitest": "0.29.2"
  },
  "authors": [
    "wangxingkang <wang_xingkang@qq.com> (https://github.com/wangxingkang)"
  ],
  "lint-staged": {
    "*.{jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
